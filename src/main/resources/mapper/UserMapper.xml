<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="icu.zeeman.exps.pojo.User">
  <select id="selectById" parameterType="int" resultType="icu.zeeman.exps.pojo.User">
    SELECT *
    FROM mybatis.users
    WHERE uid = #{uId}
  </select>
</mapper>
